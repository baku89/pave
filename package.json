{
	"name": "@baku89/pave",
	"version": "0.2.0",
	"description": "A low level toolkit for manipulating SVG/Path2D paths.",
	"main": "./lib/cjs/index.js",
	"module": "./lib/esm/index.js",
	"types": "./lib/esm/index.d.ts",
	"sideEffects": false,
	"scripts": {
		"dev": "concurrently npm:dev:api npm:dev:docs",
		"dev:api": "npm run build:api -- --watch",
		"dev:docs": "vuepress dev docs",
		"build": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json",
		"build:api": "typedoc src/index.ts",
		"build:doc": "npm run build:api; vuepress build docs",
		"lint": "eslint",
		"test": "jest",
		"prepare": "npm run build",
		"preversion": "npm run test",
		"postversion": "git push && git push --tags && npm publish --access=public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/baku89/pave.git"
	},
	"keywords": [
		"svg",
		"path",
		"transform",
		"graphics",
		"vector",
		"matrix"
	],
	"author": "Baku Hashimoto <mail@baku89.com>",
	"files": [
		"lib"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/baku89/pave/issues"
	},
	"homepage": "https://baku89.github.io/pave",
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vueuse/core": "^10.7.2",
		"concurrently": "^8.2.2",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-jest": "^27.2.3",
		"eslint-plugin-prettier-vue": "^5.0.0",
		"eslint-plugin-simple-import-sort": "^12.0.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"eslint-plugin-vue": "^9.19.2",
		"jest": "^29.7.0",
		"jest-canvas-mock": "^2.5.2",
		"jest-environment-jsdom": "^29.7.0",
		"jest-runner-eslint": "^2.1.1",
		"monaco-editor": "^0.44.0",
		"monaco-editor-vue3": "^0.1.10",
		"monaco-themes": "^0.4.4",
		"prettier": "^3.0.3",
		"safer-eval": "^1.3.6",
		"stylus": "^0.62.0",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"typedoc": "^0.25.8",
		"typedoc-plugin-markdown": "^3.17.1",
		"typescript": "^5.3.3",
		"vue": "^3.4.19",
		"vue-eslint-parser": "^9.3.2",
		"vuepress": "^2.0.0-rc.0"
	},
	"dependencies": {
		"@types/bezier-js": "^4.1.3",
		"bezier-js": "^6.1.4",
		"linearly": "0.22.0",
		"paper": "^0.12.17",
		"paperjs-offset": "^1.0.8"
	}
}
