{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": ["./src/index.ts"],
	"out": "./docs/api",
	"plugin": ["typedoc-plugin-markdown"],
	"readme": "none",
	"sort": ["alphabetical"],
	"cleanOutputDir": true,
	"includeVersion": true,
	"hideInPageTOC": true,
	"categoryOrder": [
		"Primitives",
		"Properties",
		"Modifiers",
		"Boolean Operations",
		"Converters",
		"Draw Functions",
		"Utilities",
		"*"
	]
}
